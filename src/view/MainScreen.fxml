<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreen">
    <children>
        <Button fx:id="deleteMainPartButton" layoutX="332.0" layoutY="321.0" mnemonicParsing="false" onAction="#deletePartButtonAction" text="Delete" />
        <TableView fx:id="partMainTable" layoutY="64.0" prefHeight="256.0" prefWidth="394.0">
            <columns>
                <TableColumn fx:id="leftTablePartIdCol" prefWidth="75.0" text="Part ID" />
                <TableColumn fx:id="leftTablePartNameCol" prefWidth="75.0" text="Part Name" />
                <TableColumn fx:id="leftTableInvCol" prefWidth="110.19998168945312" text="Inventory Level" />
                <TableColumn fx:id="leftTablePriceCol" prefWidth="133.00003051757812" text="Price/Cost Per Unit" />
            </columns>
        </TableView>
        <TextField fx:id="partSearchField" layoutX="245.0" layoutY="32.0" onAction="#partSearchFieldAction" promptText="Search by Part ID or Name" />
        <TextField fx:id="productSearchField" layoutX="648.0" layoutY="32.0" onAction="#productSearchFieldAction" promptText="Search by Product ID or Name" />
        <TableView fx:id="productMainTable" layoutX="400.0" layoutY="64.0" prefHeight="256.0" prefWidth="394.0">
            <columns>
                <TableColumn fx:id="rightTableProductIdCol" prefWidth="73.5999755859375" text="Product ID" />
                <TableColumn fx:id="rightTableProductNameCol" prefWidth="95.99996948242188" text="Product Name" />
                <TableColumn fx:id="rightTableInvCol" prefWidth="95.20001220703125" text="Inventory Level" />
                <TableColumn fx:id="rightTablePriceCol" prefWidth="131.20001220703125" text="Price/Cost Per Unit" />
            </columns>
        </TableView>
        <Button fx:id="addMainPartButton" layoutX="207.0" layoutY="321.0" mnemonicParsing="false" onAction="#addPartButtonAction" text="Add" />
        <Button fx:id="modifyMainPartButton" layoutX="255.0" layoutY="321.0" mnemonicParsing="false" onAction="#modifyPartButtonAction" text="Modify" />
        <Button fx:id="deleteMainProductButton" layoutX="735.0" layoutY="321.0" mnemonicParsing="false" onAction="#deleteProductButtonAction" text="Delete" />
        <Button fx:id="addMainProductButton" layoutX="610.0" layoutY="321.0" mnemonicParsing="false" onAction="#addProductButtonAction" text="Add" />
        <Button fx:id="modifyMainProductButton" layoutX="658.0" layoutY="321.0" mnemonicParsing="false" onAction="#modifyProductButtonAction" text="Modify" />
        <Label layoutX="14.0" layoutY="14.0" text="Inventory Management System">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Button fx:id="exitMainButton" layoutX="735.0" layoutY="361.0" mnemonicParsing="false" onAction="#exitMainButtonAction" text="Exit" />
    </children>
</AnchorPane>
